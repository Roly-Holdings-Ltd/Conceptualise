<link rel="stylesheet" href="{{ 'component-rte.css' | asset_url }}" media="print" onload="this.media='all'">
<link rel="stylesheet" href="{{ 'section-rich-text.css' | asset_url }}" media="print" onload="this.media='all'">
<link rel="stylesheet" href="{{ 'component-slider-v2.css' | asset_url }}" media="print" onload="this.media='all'">

<noscript>{{ 'component-rte.css' | asset_url | stylesheet_tag }}</noscript>
<noscript>{{ 'section-rich-text.css' | asset_url | stylesheet_tag }}</noscript>
<noscript>{{ 'component-slider-v2.css' | asset_url | stylesheet_tag }}</noscript>

{%- style -%}
  .section-{{ section.id }}-padding {
    padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
    padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
  }

  @media screen and (min-width: 750px) {
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }
  }
  {%- if section.settings.use_custom_colors -%}
  .section-{{ section.id }}.rich-text {
    background-color: {{ section.settings.background_color_light }};
  }
  body.theme-dark-mode .section-{{ section.id }}.rich-text {
    background-color: {{ section.settings.background_color_dark }};
  }
    
  {%- endif -%}

  {%- for block in section.blocks -%}
    {%- case block.type -%}
      {%- when 'heading' -%}
        #block--{{ block.id }}{
          font-size: {{ block.settings.heading_font_size_mobile }}px;
          color: {{ block.settings.text_color_light }};
        }
        body.theme-dark-mode #block--{{ block.id }}{
          color: {{ block.settings.text_color_dark }};
        }
        @media (min-width: 750px) {
          #block--{{ block.id }}{
            font-size: {{ block.settings.heading_font_size_desktop }}px;
          }
        }
        {%- when 'subheading' -%}
          #subheading--{{ block.id }}{
            font-size: {{ block.settings.font_size_mobile }}px;
            color: {{ block.settings.text_color_light }};
          }
          body.theme-dark-mode #block--{{ block.id }}{
            color: {{ block.settings.text_color_dark }};
          }
          @media (min-width: 750px){
            #subheading--{{ block.id }}{
              font-size: {{ block.settings.font_size_desktop }}px;
            }
          }
        {%- when 'caption' -%}
          #block--{{ block.id }}{
            font-size: {{ block.settings.caption_font_size_mobile }}px;
            color: {{ block.settings.text_color_light }};
          }
          body.theme-dark-mode #block--{{ block.id }}{
            color: {{ block.settings.text_color_dark }}
          }
          @media (min-width: 750px) {
            #block--{{ block.id }}{
              font-size: {{ block.settings.caption_font_size_desktop }}px;
            }
          }
          {%- when 'text' -%}
            #block--{{ block.id }} *,
            #block--{{ block.id }}{
              font-size: {{ block.settings.font_size_mobile }}px;
              color: {{ block.settings.text_color_light }};
            }
            body.theme-dark-mode #block--{{ block.id }} *,
            body.theme-dark-mode #block--{{ block.id }} {
              color: {{ block.settings.text_color_dark }}
            }
            @media (min-width: 750px) {
              #block--{{ block.id }} *,
              #block--{{ block.id }}{
                font-size: {{ block.settings.font_size_desktop }}px;
              }
            }
          {%- when 'single_image' -%}
            #block--{{ block.id }}.rich-text__single-image{
                max-width: {{ block.settings.image_max_width }}px;
            }

          {%- when 'image_grid' -%}
          #block--{{ block.id }}.rich-text__image-grid .rich-text__image-grid-item-heading p,
          #block--{{ block.id }}.rich-text__image-grid .rich-text__image-grid-item-content-column-heading {
            color: {{ block.settings.headings_color_light }};
          }
          body.theme-dark-mode #block--{{ block.id }}.rich-text__image-grid .rich-text__image-grid-item-heading p,
          body.theme-dark-mode #block--{{ block.id }}.rich-text__image-grid .rich-text__image-grid-item-content-column-heading {
            color: {{ block.settings.headings_color_dark }};
          }

          #block--{{ block.id }}.rich-text__image-grid .rich-text__image-grid-item-text-content p,
          #block--{{ block.id }}.rich-text__image-grid .rich-text__image-grid-item-content-column-subheading {
            color: {{ block.settings.subheadings_color_light }};
          }
          body.theme-dark-mode #block--{{ block.id }}.rich-text__image-grid .rich-text__image-grid-item-text-content p,
          body.theme-dark-mode #block--{{ block.id }}.rich-text__image-grid .rich-text__image-grid-item-content-column-subheading {
            color: {{ block.settings.subheadings_color_dark }};
          }
    {%- endcase -%}
  {%- endfor -%}
{%- endstyle -%}

<div class="isolate{% unless section.settings.full_width %} page-width{% endunless %}">
  <div class="section-{{ section.id }} rich-text content-container color-{{ section.settings.color_scheme }} gradient{% if section.settings.full_width %} rich-text--full-width content-container--full-width{% endif %} section-{{ section.id }}-padding">
    <div class="rich-text__wrapper rich-text__wrapper--{{ section.settings.desktop_content_position }}{% if section.settings.full_width %} page-width{% endif %}">
      <div class="rich-text__blocks {{ section.settings.content_alignment }}">
        {%- for block in section.blocks -%}
          {%- case block.type -%}
            {%- when 'heading' -%}
              <h2 id="block--{{ block.id }}" class="rich-text__heading rte {{ block.settings.heading_size }}" {{ block.shopify_attributes }}>
                {{ block.settings.heading | replace: 'p>', 'span>' }}
              </h2>
              {%- when 'subheading' -%}
              <div id="subheading--{{ block.id }}" class="rich-text__subheading rte" {{ block.shopify_attributes }}>
                {{ block.settings.subheading | replace: 'p>', 'span>' }}
              </div>
            {%- when 'caption' -%}
              <p id="block--{{ block.id }}" class="rich-text__caption {{ block.settings.text_style }} {{ block.settings.text_style }}--{{ block.settings.text_size }}" {{ block.shopify_attributes }}>
                {{ block.settings.caption | escape }}
              </p>
            {%- when 'text' -%}
              <div id="block--{{ block.id }}" class="rich-text__text rte" {{ block.shopify_attributes }}>
                {{ block.settings.text }}
              </div>
            {%- when 'button' -%}
              <div class="rich-text__buttons{% if block.settings.button_label != blank and block.settings.button_label_2 != blank %} rich-text__buttons--multiple{% endif %}" {{ block.shopify_attributes }}>
                {%- if block.settings.button_label != blank -%}
                  <a{% if block.settings.button_link == blank %} role="link" aria-disabled="true"{% else %} href="{{ block.settings.button_link }}"{% endif %} class="button{% if block.settings.button_style_secondary %} button--secondary{% else %} button--primary{% endif %}">{{ block.settings.button_label | escape }}</a>
                {%- endif -%}
                {%- if block.settings.button_label_2 != blank -%}
                  <a{% if block.settings.button_link_2 == blank %} role="link" aria-disabled="true"{% else %} href="{{ block.settings.button_link_2 }}"{% endif %} class="button{% if block.settings.button_style_secondary_2 %} button--secondary{% else %} button--primary{% endif %}">{{ block.settings.button_label_2 | escape }}</a>
                {%- endif -%}
              </div>
            {%- when 'single_image' -%}
              <div id="block--{{ block.id }}" class="rich-text__single-image">
                {%- if block.settings.image_light != blank -%}
                    <img
                        srcset="{%- if block.settings.image_light.width >= 165 -%}{{ block.settings.image_light | image_url: width: 165 }} 165w,{%- endif -%}
                            {%- if block.settings.image_light.width >= 360 -%}{{ block.settings.image_light | image_url: width: 360 }} 360w,{%- endif -%}
                            {%- if block.settings.image_light.width >= 535 -%}{{ block.settings.image_light | image_url: width: 535 }} 535w,{%- endif -%}
                            {%- if block.settings.image_light.width >= 750 -%}{{ block.settings.image_light | image_url: width: 750 }} 750w,{%- endif -%}
                            {{ block.settings.image_light | image_url }} {{ block.settings.image_light.width }}w"
                        src="{{ block.settings.image_light | image_url: width: 360 }}"
                        sizes="(min-width: {{ settings.page_width }}px) {{ settings.page_width | minus: 100 | divided_by: 2 }}px, (min-width: 750px) calc((100vw - 130px) / 2), calc((100vw - 50px) / 2)"
                        alt="{{ block.settings.image_light.alt | escape }}"
                        loading="lazy"
                        width="{{ block.settings.image_light.width }}"
                        height="{{ block.settings.image_light.height }}"
                        class="rich-text__single-image--light"
                    >
                {%- endif -%}
                {%- if block.settings.image_dark != blank -%}
                    <img
                        srcset="{%- if block.settings.image_dark.width >= 165 -%}{{ block.settings.image_dark | image_url: width: 165 }} 165w,{%- endif -%}
                            {%- if block.settings.image_dark.width >= 360 -%}{{ block.settings.image_dark | image_url: width: 360 }} 360w,{%- endif -%}
                            {%- if block.settings.image_dark.width >= 535 -%}{{ block.settings.image_dark | image_url: width: 535 }} 535w,{%- endif -%}
                            {%- if block.settings.image_dark.width >= 750 -%}{{ block.settings.image_dark | image_url: width: 750 }} 750w,{%- endif -%}
                            {{ block.settings.image_dark | image_url }} {{ block.settings.image_dark.width }}w"
                        src="{{ block.settings.image_dark | image_url: width: 360 }}"
                        sizes="(min-width: {{ settings.page_width }}px) {{ settings.page_width | minus: 100 | divided_by: 2 }}px, (min-width: 750px) calc((100vw - 130px) / 2), calc((100vw - 50px) / 2)"
                        alt="{{ block.settings.image_dark.alt | escape }}"
                        loading="lazy"
                        width="{{ block.settings.image_dark.width }}"
                        height="{{ block.settings.image_dark.height }}"
                        class="rich-text__single-image--dark"
                    >
                {%- endif -%}
            </div>
            {%- when 'image_grid' -%}
            <div id="block--{{ block.id }}" class="rich-text__image-grid">
              <slider-component-v2 id="GalleryViewer-{{ section.id }}" class="slider-mobile-gutter">
                <ul id="Slider-Gallery-{{ section.id }}" class="rich-text__image-grid-items grid grid--peek list-unstyled slider slider-v2 slider--mobile desktop--{{ block.settings.number_of_cols_desktop }}-cols" role="list">
                  {%- if block.settings.show_item1 -%}
