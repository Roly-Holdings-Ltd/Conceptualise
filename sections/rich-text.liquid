                                  {%- if block.settings.item1_image.width >= 535 -%}{{ block.settings.item1_image | image_url: width: 535 }} 535w,{%- endif -%}
                                  {%- if block.settings.item1_image.width >= 750 -%}{{ block.settings.item1_image | image_url: width: 750 }} 750w,{%- endif -%}
                                  {{ block.settings.item1_image | image_url }} {{ block.settings.item1_image.width }}w"
                              src="{{ block.settings.item1_image | image_url: width: 360 }}"
