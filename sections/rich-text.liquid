                              alt="{{ block.settings.item1_image.alt | escape }}"
                              loading="lazy"
                              width="{{ block.settings.item1_image.width }}"
                              height="{{ block.settings.item1_image.height }}"
                          >
                        </div>
                      {%- endif -%}
                      {%- if block.settings.item1_heading != blank -%}
                        <div class="rich-text__image-grid-item-heading">
                          <p>{{ block.settings.item1_heading }}</p>
                        </div>
                      {%- endif -%}
                      {%- if block.settings.item1_text_content -%}
                        <div class="rich-text__image-grid-item-text-content">
                          <p>{{ block.settings.item1_text_content }}</p>
                        </div>
                      {%- endif -%}
              
                      {%- if block.settings.show_item1_content_columns -%}
