{%- style -%}
{%- for block in section.blocks -%}
    {%- case block.type -%}
        {%- when 'content_text' -%}
        #block--{{ block.id }} p {
            font-size: {{ block.settings.font_size_mobile }}px;
            font-weight: {{ block.settings.font_weight }};
            font-style: {{ block.settings.font_style }};
        }
        {% if block.settings.use_custom_text_color %}
            #block--{{ block.id }} p {
                color: {{ block.settings.text_color_light }};
            }
            body.theme-dark-mode #block--{{ block.id }} p {
                color: {{ block.settings.text_color_dark }};
            }
        {% endif %}

        @media (min-width: 750px){
            #block--{{ block.id }} p {
                font-size: {{ block.settings.font_size_desktop }}px;
            }
        }
        
        {%- when 'content_text_heading' -%}          
        #block--{{ block.id }}.main-banner__content-info-row h1 {
            font-size: {{ block.settings.font_size_mobile }}px;
        }
        #block--{{ block.id }}.main-banner__content-info-row img {
            height: {{ block.settings.image_size_mobile }}px;
            width: auto;
        }
        {%- if block.settings.image != blank -%}
                #block--{{ block.id }}.main-banner__content-info-row h1{
                    overflow: visible;
                    position: relative;
                }
                #block--{{ block.id }}.main-banner__content-info-row h1:before {
                    content: " ";
                    position: absolute;
                    width: {{ block.settings.image_width_mobile }}px;
                    height: {{ block.settings.image_height_mobile }}px;
                    margin-top: {{ block.settings.image_vertical_position_mobile }}px;
                    background-image: url("{{ block.settings.image | img_url: 'medium' }}");
                    background-size: 100% 100%;
                    
                    {%- if block.settings.image_position == 'before' -%}
                        left: 0;
                        margin-left: {{ block.settings.image_horizontal_position_mobile }}px;
                    {%- else -%}
                        right: 0;
                        margin-right: {{ block.settings.image_horizontal_position_mobile }}px;
                    {%- endif -%}
                    
                }
            {%- endif -%}

            {% if block.settings.use_custom_text_color %}
            #block--{{ block.id }}.main-banner__content-info-row h1{
                color: {{ block.settings.text_color_light }};
            }
            body.theme-dark-mode #block--{{ block.id }}.main-banner__content-info-row h1{
                color: {{ block.settings.text_color_dark }};
            }
            {% endif %}
        @media (min-width: 750px){
            #block--{{ block.id }}.main-banner__content-info-row h1 {
                font-size: {{ block.settings.font_size_desktop }}px;
            }
            #block--{{ block.id }}.main-banner__content-info-row img {
                height: {{ block.settings.image_size_desktop }}px;
                width: auto;
            }
            {%- if block.settings.image != blank -%}
                #block--{{ block.id }}.main-banner__content-info-row h1:before {
                    width: {{ block.settings.image_width_desktop }}px;
                    height: {{ block.settings.image_height_desktop }}px;
                    margin-top: {{ block.settings.image_vertical_position_desktop }}px;
                    background-image: url("{{ block.settings.image | img_url: 'medium' }}");
                    background-size: 100% 100%;

                    {%- if block.settings.image_position == 'before' -%}
                        margin-left: {{ block.settings.image_horizontal_position_desktop }}px;
                    {%- else -%}
                        margin-right: {{ block.settings.image_horizontal_position_desktop }}px;
                    {%- endif -%}
                }
            {%- endif -%}
        }
        {%- when 'content_button' -%}

        #block--{{ block.id }}.main-banner__content-info-row-button a {
            font-size: {{ block.settings.font_size_mobile }}px;
        }
        @media (min-width: 750px){
            #block--{{ block.id }}.main-banner__content-info-row-button a {
                font-size: {{ block.settings.font_size_desktop }}px;
            }
        }

        {%- when 'statistics' -%}
        #block--{{ block.id }}.main-banner__statistics .main-banner__statistic-subheading {
            font-size: {{ block.settings.subheading_font_size_mobile }}px;
        }
        #block--{{ block.id }}.main-banner__statistics .main-banner__statistic-heading {
            font-size: {{ block.settings.heading_font_size_mobile }}px;
        }
        @media (min-width: 750px){
            #block--{{ block.id }}.main-banner__statistics .main-banner__statistic-subheading {
                font-size: {{ block.settings.subheading_font_size_desktop }}px;
            }
            #block--{{ block.id }}.main-banner__statistics .main-banner__statistic-heading {
                font-size: {{ block.settings.heading_font_size_desktop }}px;
            }
        }

        {%- when 'logo-row' -%}
        #block--{{ block.id }}.main-banner__logo-row img{
            <!-- max-height: {{ block.settings.image_max_height }}px; -->
        }
        @media (max-widt:750px){
